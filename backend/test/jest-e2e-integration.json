{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": "..",
  "testMatch": ["**/*.integration-spec.ts"],
  "testEnvironment": "node",
  "transform": {"^.+\\.(t|j)s$": "ts-jest"},
  "transformIgnorePatterns": ["/node_modules/(?!uuid/)"],
  "setupFilesAfterEnv": ["<rootDir>/test/e2e/setup.ts"],
  "verbose": true
}
